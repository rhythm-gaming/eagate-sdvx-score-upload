{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "strict": true,
        "moduleResolution": "node",
        "isolatedModules": false,
        "paths": {
            "@/*": ["./src/*"]
        },
        "noEmit": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
}